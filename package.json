{
  "name": "pet-bet",
  "type": "module",
  "version": "0.1.0",
  "description": "A handy app to determine most popular pet",
  "repository": "https://https://github.com/manaia-2023/what-day-is-it",
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite --host",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "test": "vitest",
    "test-coverage": "vitest -- --coverage",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:ci": "eslint --ext .js,.jsx,.ts,.tsx . --rule '@typescript-eslint/no-unused-vars: off'",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "keywords": [
    "fullstack"
  ],
  "author": "Dev Academy Aotearoa",
  "license": "ISC",
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react"
  },
  "dependencies": {
    "@cloudinary/react": "^1.14.3",
    "@cloudinary/url-gen": "^1.21.0",
    "cloudinary": "^1.41.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "http-status-codes": "^2.3.0",
    "jwks-rsa": "^3.1.0",
    "knex": "^2.5.1",
    "lucide-react": "^0.516.0",
    "multer": "^2.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "react-dropzone": "^14.3.8",
    "sqlite3": "^5.1.7",
    "superagent": "^10.2.1",
    "swiper": "^11.2.8"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@devacademy/eslint-config": "^1.10.1",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/themes": "^2.0.1",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^8.1.7",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.19.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "nock": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.6.2",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
